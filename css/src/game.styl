*
	margin 0
	padding 0
	box-sizing border-box


body
	background #000 url('/images/bg_body.jpg') 50% 0 repeat
	// background #fff
	font-family: 'Architects Daughter', cursive, Arial;

h1
	font-size 40px
	font-weight normal

.game
	margin 100px auto 0
	text-align center
	color #fff
	user-select none


.maze
	border 10px solid #946538
	display inline-block
	margin 50px auto 0
	& canvas
		margin auto
